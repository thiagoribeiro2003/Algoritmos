algoritmo "LOOP BANDAS"
// Função :
// Autor :
// Data : 04/03/2022
// Seção de Declarações 
var
contador: inteiro
artista: caractere
inicio
// Seção de Comandos
para contador de 1 ate 5 faca

//ENTRADAS
escreva("Informe a",contador,"ª banda/artista: ")
leia(artista)


//SAÍDA
escreval("A banda informada foi: ", artista)
escreval()




fimpara

 
fimalgoritmo
